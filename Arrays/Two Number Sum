#include <vector>
#include <algorithm>
using namespace std;


vector<int> twoNumberSum(vector<int> array, int targetSum) {
  int n1, n2;
	for(int i=0; i< array.size()-1; i++)
	{
		n1 = array[i];
		n2 = targetSum - n1;
		if(n1 == n2)
			continue;
		else if(std :: find(array.begin(), array.end(), n2) != array.end())
			return vector<int>{n1, n2};
		else
			continue;
	}
		return {};
}
